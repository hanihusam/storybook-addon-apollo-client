{
  "name": "storybook-addon-apollo-client",
  "version": "9.0.0",
  "description": "Use Apollo Client in your Storybook stories.",
  "keywords": [
    "storybook-addon",
    "data-state",
    "apollo",
    "popular",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeiscontent/storybook-addon-apollo-client"
  },
  "type": "module",
  "license": "MIT",
  "author": "Aaron Reisman <aaron@lifeiscontent.net>",
  "engines": {
    "node": ">=20.19.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "default": "./dist/preview.js"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "node -e \"fs.rmSync('./dist', { recursive: true, force: true })\"",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "format": "prettier --write .",
    "release": "npm run build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@apollo/client": "^3.13.8",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-links": "^10.0.0",
    "@storybook/react-vite": "^10.0.0",
    "@types/node": "22.13.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "auto": "11.3.0",
    "boxen": "8.0.1",
    "dedent": "1.6.0",
    "esbuild": "^0.25.5",
    "graphql": "^16.11.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.0",
    "prompts": "2.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rollup": "4.44.0",
    "storybook": "^10.0.0",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "zx": "8.6.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "graphql": "^16.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "storybook": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/index.ts",
      "src/preview.ts"
    ],
    "nodeEntries": []
  },
  "storybook": {
    "displayName": "Apollo Client",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://raw.githubusercontent.com/apollographql/apollo-client-devtools/main/assets/apollo-wordmark.svg"
  }
}
